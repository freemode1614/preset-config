{
  "name": "@moccona/preset-config",
  "version": "0.0.5",
  "description": "A set of preset configs for standardizing and streamlining configuration across projects. Includes pre-configured settings for common tools like TypeScript, Prettier, and Browserslist to ensure consistent development workflows with minimal setup.",
  "type": "module",
  "main": "./npm/index.js",
  "module": "./npm/index.js",
  "types": "./npm/index.d.ts",
  "keywords": [
    "prettierrc",
    "tsconfig"
  ],
  "files": [
    "npm",
    "tsconfig"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "freemode1614 <freemode1614@outlook.com>",
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    "./tsconfig/*": {
      "import": "./tsconfig/*",
      "require": "./tsconfig/*"
    },
    "./*": {
      "types": "./npm/*.d.ts",
      "import": "./npm/*.js",
      "require": "./npm/*.cjs"
    },
    ".": {
      "types": "./npm/index.d.ts",
      "import": "./npm/index.js",
      "require": "./npm/index.cjs"
    }
  },
  "scripts": {
    "test": "vitest --watch false",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@types/node": "^20.17.14",
    "browserslist": "^4.28.0",
    "prettier": "^3.7.3",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^3.0.2"
  },
  "packageManager": "pnpm@8.15.3"
}
